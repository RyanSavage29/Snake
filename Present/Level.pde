void checkSolid(int direction)
{
  switch (direction)
  {
    case 0:
    if (player1.yPos - 1 < 0 || level[player1.yPos - 1][player1.xPos] >= 6)
    {
     solid = true; 
    }
    else
    {
      solid = false;
    }
    break;
    
    case 1:
    if (player1.xPos - 1 < 0 || level[player1.yPos][player1.xPos - 1] >= 6)
    {
     solid = true; 
    }
    else
    {
      solid = false;
    }
    break;
    
    case 2:
    if (player1.yPos + 1 >= level.length || level[player1.yPos + 1][player1.xPos] >= 6)
    {
     solid = true; 
    }
    else
    {
      solid = false;
    }
    break;
    
    case 3:
    if (player1.xPos + 1 >= level.length || level[player1.yPos][player1.xPos + 1] >= 6)
    {
     solid = true; 
    }
    else
    {
      solid = false;
    }
    break;
  }
}

