void renderGridCell(int col, int row, int tileRow, int tileCol, int level[][])
{
  if (col < 0 || col >= level.length || row < 0 || row >= level.length)
      {
      image(tile[6], tileRow*tileWidth, tileCol*tileHeight);
      }
      else
      {
      image(tile[level[col][row]], tileRow*tileWidth, tileCol*tileHeight);
      }
}

void renderPlayer(int col, int row, int tileRow, int tileCol, int direction)
{
  if (animationTimer.running == false)
  {
  animationTimer.startStopWatch();
  }
  
  player1.animation();
  
  if (player1.xPos == row && player1.yPos == col)
  {
  switch(direction)
  {
    case 0:
    image(north[player1.time], (tileRow*tileWidth), (tileCol*tileHeight));
    break;
    
    case 1:
    image(west[player1.time], (tileRow*tileWidth), (tileCol*tileHeight));
    break;
    
    case 2:
    image(south[player1.time], (tileRow*tileWidth), (tileCol*tileHeight));
    break;
    
    case 3:
    image(east[player1.time], (tileRow*tileWidth), (tileCol*tileHeight));
    break;
  }
  }
}
